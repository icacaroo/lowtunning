@model lowtunning.Models.Usuarios
@{
    ViewBag.Title = "ExcluirFunc";
}
<h2>Deletar Funcionário</h2>
<div class="form-horizontal">
    <div class="form-group">
        @Html.Label("Nome Funcionário", new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.usuario,
                                      new { @class = "form-control", @disabled = "disabled" })
        </div>
    </div>
    <div class="form-group">
        @Html.Label("CPF", new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.cpf,
                                      new { @class = "form-control", @disabled = "disabled" })
        </div>
    </div>
    <div class="form-group">
        @Html.Label("Endereço", new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.endereço,
                                      new { @class = "form-control", @disabled = "disabled" })
        </div>
    </div>
    <div class="form-group">
        @Html.Label("Email", new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.email,
                                      new { @class = "form-control", @disabled = "disabled" })
        </div>
    </div>
    <div class="form-group">
        @Html.Label("Telefone", new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.telefone,
                                      new { @class = "form-control", @disabled = "disabled" })
        </div>
    </div>
</div>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Deletar Funcionário" class="btn btn-danger" />
            <br />
            @Html.ActionLink("Cancelar", "Funcionarios", "Home", new { @class = "btn btn-warning" })
        </div>
    </div>
}
